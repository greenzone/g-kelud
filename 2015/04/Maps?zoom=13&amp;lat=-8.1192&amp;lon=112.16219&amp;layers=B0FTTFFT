var map = L.map('map', {
  worldCopyJump: true,
  minZoom: 1,
  maxZoom: 10
}).setView([51.520371, 42.916844], 3);

var owm = new WeatherLayers(map);
var attribution = "Open Weather Map 2015 | Design by <a href='http://urbica.co' target='_blank'>Urbica</a>";

var basemap = new L.tileLayer("http://{s}.maps.owm.io/base/gray/{z}/{x}/{y}?appid=b1b15e88fa797225412429c1c50c122a", {
  attribution: attribution,
  maxZoom: 18,
  zIndex: 0,
  subdomains: 'abcdefgh',
  showWeather: true
}).addTo(map);

var clouds = new L.tileLayer("http://{s}.maps.owm.io/current/CLOUDS_STYLE/{z}/{x}/{y}?appid=b1b15e88fa797225412429c1c50c122a", {
  attribution: attribution,
  maxZoom: 18,
  zIndex: 1,
  scaleParams: {
    width: '300px',
    siText: '%',
    avgValue: 50,
    gradient: scales.CLOUDS_STYLE
  }
});

var precipitation = new L.tileLayer("http://{s}.maps.owm.io/current/PRECIPITATION_STYLE/{z}/{x}/{y}?appid=b1b15e88fa797225412429c1c50c122a", {
  attribution: attribution,
  maxZoom: 18,
  zIndex: 1,
  subdomains: 'abcdefgh',
  scaleParams: {
    width: '300px',
    siText: 'mm',
    avgValue: 100,
    gradient: scales.PRECIPITATION_STYLE
  }
  
});

var pressure = new L.tileLayer("http://{s}.maps.owm.io/current/PRESSURE_STYLE/{z}/{x}/{y}?appid=b1b15e88fa797225412429c1c50c122a", {
  attribution: attribution,
  maxZoom: 18,
  zIndex: 1,
  subdomains: 'a',
  scaleParams: {
    width: '300px',
    siText: 'hPa',
    avgValue: 1013.25,
    gradient: scales.PRESSURE_STYLE
  }
});

var rain = new L.tileLayer("http://{s}.maps.owm.io/current/RAIN_STYLE/{z}/{x}/{y}?appid=b1b15e88fa797225412429c1c50c122a", {
  attribution: attribution,
  maxZoom: 18,
  zIndex: 1,
  scaleParams: {
    width: '300px',
    siText: 'mm',
    avgValue: 100,
    gradient: scales.RAIN_STYLE
}
});

var snow = new L.tileLayer("http://{s}.maps.owm.io/current/SNOW_STYLE/{z}/{x}/{y}?appid=b1b15e88fa797225412429c1c50c122a", {
  attribution: attribution,
  maxZoom: 18,
  zIndex: 1,
  scaleParams: {
    width: '300px',
    siText: 'mm',
    avgValue: 100,
    gradient: scales.SNOW_STYLE
}
});


var temperature = new L.tileLayer("http://{s}.maps.owm.io/current/TEMP_STYLE/{z}/{x}/{y}?appid=b1b15e88fa797225412429c1c50c122a", {
  attribution: attribution,
  maxZoom: 18,
  zIndex: 1,
   scaleParams: {
    width: '300px',
    siText: '°C',
    avgValue: 0,
    gradient: scales.TEMP_STYLE
 }    
});

var windspeed = new L.tileLayer("http://{s}.maps.owm.io/current/WINDSPEED_STYLE/{z}/{x}/{y}?appid=b1b15e88fa797225412429c1c50c122a", {
  attribution: attribution,
  maxZoom: 18,
  zIndex: 1,
  scaleParams: {
    width: '300px',
    siText: 'm/s',
    avgValue: 100,
    gradient: scales.WINDSPEED_STYLE
}
});



var prs = new L.tileLayer("http://{s}.maps.owm.io/weather/pressure_cntr/{z}/{x}/{y}.png?appid=b1b15e88fa797225412429c1c50c122a", {
  attribution: attribution,
  maxZoom: 18,
  zIndex: 1
});


var overlays = {
//    "prs":prs
};

var baseLayers = {
  "Base map": basemap,
  "Clouds": clouds,
  "Precipitation": precipitation,
  "Pressure": pressure,
  "Rain": rain,
  "Snow": snow,
  "Temperature": temperature,
  "Wind speed": windspeed
};

var control = owm.getCustomLayersControl(baseLayers, overlays);
map.addControl(control);


var town = owm.getCitiesLayer('http://{s}.maps.owm.io/weather/cities/{z}/{x}/{y}.geojson?appid=b1b15e88fa797225412429c1c50c122a');
var weathermap = owm.getWeatherLayer('http://{s}.maps.owm.io/weather/cities/{z}/{x}/{y}.geojson?appid=b1b15e88fa797225412429c1c50c122a').addTo(map);
owm.bindCities('showWeather', weathermap, town);
owm.bindScales();    
    
</script>


