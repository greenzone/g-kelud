<script type="text/javascript">
jQuery(document).ready(function() {

  var units = 'metric';
  if( get_cookie('units') ) units = get_cookie('units');
  if( units == 'metric') {
      document.getElementById("units_check").checked = true;
  }else{
      document.getElementById("units_check").checked = false;
  }
});
</script>
    
    
